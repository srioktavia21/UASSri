<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.informatika19100073.srioktavia_19100073_alarm">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Alarm"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.informatika19100073.srioktavia_19100073_alarm.LoginActivity"
            android:label="Login Activity"></activity>
        <activity
            android:name="com.informatika19100073.srioktavia_19100073_alarm.InsertDataActivity"
            android:label="InsertDataActivity"></activity>
        <activity
            android:name="com.informatika19100073.srioktavia_19100073_alarm.UpdateDataActivity"
            android:label="Update Data Activity"></activity>
        <activity android:name=".MainActivity"/>
        <activity
            android:name=".SplashScreen"
            android:label="Splash Screen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>